{
    "workbench.startupEditor": "none",
    "workbench.fontAliasing": "auto",
    "workbench.sideBar.location": "left",
    "editor.fontFamily": "Fira Code",
    "editor.fontSize": 10,
    "editor.lineHeight": 13,
    "editor.fontLigatures": true,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "editor.renderLineHighlight": "all",
    "editor.rulers": [80, 110],
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
    "editor.wordWrap": "off",
    "editor.detectIndentation": true,
    "editor.autoClosingBrackets": "always",
    "editor.minimap.enabled": false,
    "editor.codeActionsOnSave": {
      "source.fixAll": true,
      "source.organizeImports": false,
    },
    "files.trimTrailingWhitespace": true,
    "files.associations": {
      "*.tpl": "html"
    },
    "git.enabled": true,
    "git.path": "/usr/bin/git",
    "go.buildTags": "",
    "go.buildFlags": [],
    "go.lintTool": "golint",
    "go.lintFlags": [],
    "go.vetFlags": [],
    "go.coverOnSave": false,
    "go.useCodeSnippetsOnFunctionSuggest": false,
    "go.formatFlags": [],
    "go.gopath": "/Users/bruno/Workspace/go",
    "go.gocodeAutoBuild": false,
    "python.linting.enabled": true,
    "python.linting.lintOnSave": true,
    "python.linting.pylintCategorySeverity.convention": "Hint",
    "python.linting.pylintCategorySeverity.warning": "Error",
    "window.title": "${rootName}${separator}${activeEditorShort}${separator}${activeEditorMedium}",
    "terminal.integrated.fontFamily": "Fira Code",
    "[javascript]": {
      "editor.tabSize": 2,
      "editor.formatOnSave": true,
    },
    "[json]": {
      "editor.tabSize": 2,
    },
    "[python]": {
      "editor.tabSize": 4,
      "editor.insertSpaces": true,
      "editor.formatOnSave": true
    },
    "[go]": {
      "editor.formatOnSave": true
    },
    "telemetry.telemetryLevel": "off",
    "eslint.alwaysShowStatus": true,
    "eslint.packageManager": "yarn",
    "eslint.enable": true,
    "terminal.integrated.fontWeightBold": "normal",
    "terminal.external.osxExec": "/usr/local/bin/fish",
    "highlight.regexes": {
      "(// ?TODO:?)(.*)": [
        {
          "overviewRulerColor": "#ffcc00",
          "backgroundColor": "#ffcc00",
          "color": "#1f1f1f",
          "fontWeight": "bold"
        },
        {
          "backgroundColor": "#d9ad00",
          "color": "#1f1f1f"
        }
      ],
      "(// ?FIXME:?)(.*)": [
        {
          "overviewRulerColor": "#ff0000",
          "backgroundColor": "#ff0000",
          "color": "#1f1f1f",
          "fontWeight": "bold"
        },
        {
          "backgroundColor": "#d90000",
          "color": "#1f1f1f"
        }
      ],
      "(// )(@\\w+)": [
        {},
        {
          "color": "#4de0ff"
        }
      ]
    },
    "editor.bracketPairColorization.enabled": true,
    "workbench.iconTheme": "vscode-icons",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "vsicons.dontShowNewVersionMessage": true,
    "files.watcherExclude": {
      "**/.bloop": true,
      "**/.metals": true,
      "**/.ammonite": true
    },
    "redhat.telemetry.enabled": false,
    "codesnap.showWindowControls": false,
    "codesnap.shutterAction": "copy",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
      "**/.classpath": true,
      "**/.project": true,
      "**/.settings": true,
      "**/.factorypath": true
    },
    "window.zoomLevel": 1,
    "workbench.colorTheme": "Dracula Pro (Van Helsing)"
  }
